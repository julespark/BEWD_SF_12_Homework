<% if @user.errors.any? %>
  <h2>There were errors submitting your user. Remember premium can only be 1 or 2</h2>
  <ul>
    <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>



<%= form_for @user do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %><br/>
 <%= f.label :premium_member %>
 <%= f.text_field :premium_member %>
 <%= f.submit %>
<% end %>